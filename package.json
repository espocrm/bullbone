{
  "name": "bullbone",
  "version": "1.2.2",
  "description": "A library for EspoCRM frontend.",
  "main": "dist/bullbone.js",
  "browser": "dist/bullbone.umd.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-umd": "npx rollup src/bullbone.js --format umd --file dist/bullbone.umd.js --name bullbone --amd.id bullbone",
    "build-esm": "npx rollup src/bullbone.js --format esm --file dist/bullbone.js",
    "prepare": "npm run build-umd && npm run build-esm"
  },
  "repository": "https://github.com/yurikuzn/bull",
  "author": "Yuri Kuznersov & EspoCRM team",
  "license": "MIT",
  "devDependencies": {
    "grunt": "^1.5.3",
    "grunt-contrib-concat": "~1.0.1",
    "handlebars": "^4.7.7",
    "jquery": "^3.6.0",
    "underscore": "^1.13.4"
  }
}
